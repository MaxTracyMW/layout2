classdef tGet < matlab.unittest.TestCase
    %TGET Tests for uiextras.get.

    methods ( Test, TestTags = {'uiextras.get'} )

        function obj = tGetErrorsWithNotExactlyTwoInputs( testCase )

            % Test that fewer than two input arguments causes an error.
            f = @() uiextras.get();
            testCase.verifyError( f, ...
                'MATLAB:narginchk:notEnoughInputs', ...
                'uiextras.get has accepted fewer than two inputs.' )

            % Test that more than two input arguments causes an error.
            f = @() uiextras.get( 0, 0, 0 );
            testCase.verifyError( f, ...
                'MATLAB:TooManyI')


            
        end       

    end % methods ( Test, TestTags = {'uiextras.get'} )

end % class

